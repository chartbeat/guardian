{
  "name": "My First Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "extension"
  ],
  "background": {
    "scripts": ["background.js", "js/jquery-1.7.2.min.js"]
  },
  "content_scripts": [
  {
     "matches": ["<all_urls>"],
     "js": ["content.js"]
  }
  ]
}